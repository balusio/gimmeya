{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "settings": {
    "import/resolver": {
      "webpack": {
        "config" : "tasks/webpack.config.dev.js"
      }
    }
  },
  "rules": {
    "react/prefer-stateless-function": 0
  },
  "env": {
    "browser": true,
    "node": true
  }
}
